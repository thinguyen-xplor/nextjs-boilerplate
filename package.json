    "eslint": "7.30.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "extract-react-intl-messages": "^4.1.1",
    "husky": "^7.0.1",
